package org.droiddraw;

import java.io.IOException;

public class JnlpMain {
	// Hacky main for JNLP version.  Set --is_jnlp flag and then
	// delegate.
	public static void main(String[] argsIn) {
		String[] args = new String[] { "--is_jnlp" };
		try {
			Main.main(args);
		} catch (IOException ex) {
			ex.printStackTrace();
		}
	}
}
